{
  "name": "parcel-bug-20180121",
  "version": "1.0.0",
  "description": "Website for enPiT everi",
  "main": "index.js",
  "repository": "git@github.com:zacky1972/parcel-bug-20180121.git",
  "author": "Susumu Yamazaki <zacky1972@gmail.com>",
  "dependencies": {
    "gulp": "^3.9.1",
    "gulp-coffee": "^3.0.2",
    "gulp-parcel": "^0.3.3",
    "jquery": "^3.3.0",
    "parcel-bundler": "^1.4.1"
  },
  "scripts": {
    "setup": "bundle install && yarn install",
    "build": "bundle exec middleman build && yarn fix-html && yarn bundle",
    "fix-html": "sed -i '' 's/=\"[/]\\([^/]\\)/=\".\\/\\1/g' build/index.html",
    "bundle": "yarn parcel build build/index.html --no-minify --out-dir dist --public-url ./",
    "dev": "yarn parcel index.html --out-dir dist --public-url ./"
  },
  "license": "MIT"
}
